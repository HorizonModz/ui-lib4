-- Key System
local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
screenGui.Name = "KeySystem"

local frame = Instance.new("Frame", screenGui)
frame.Size = UDim2.new(0, 200, 0, 160)
frame.Position = UDim2.new(0.5, -100, 0.5, -80)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
frame.BorderSizePixel = 0

local textbox = Instance.new("TextBox", frame)
textbox.Size = UDim2.new(1, -20, 0, 40)
textbox.Position = UDim2.new(0, 10, 0, 10)
textbox.PlaceholderText = "Enter key..."
textbox.Text = ""
textbox.TextColor3 = Color3.new(1, 1, 1)
textbox.BackgroundColor3 = Color3.fromRGB(50, 50, 50)

local submitButton = Instance.new("TextButton", frame)
submitButton.Size = UDim2.new(1, -20, 0, 40)
submitButton.Position = UDim2.new(0, 10, 0, 60)
submitButton.Text = "Submit"
submitButton.TextColor3 = Color3.new(1, 1, 1)
submitButton.BackgroundColor3 = Color3.fromRGB(60, 120, 60)

local copyButton = Instance.new("TextButton", frame)
copyButton.Size = UDim2.new(1, -20, 0, 30)
copyButton.Position = UDim2.new(0, 10, 0, 110)
copyButton.Text = "Get Key [Clipboard]"
copyButton.TextColor3 = Color3.new(1, 1, 1)
copyButton.BackgroundColor3 = Color3.fromRGB(60, 120, 60)

submitButton.MouseButton1Click:Connect(function()
	if textbox.Text == "1234" then
		loadstring(game:HttpGet("https://raw.githubusercontent.com/SkibidiMonkey52/nothinglol/refs/heads/main/sl2"))()
		submitButton.Text = "Access Granted"
        frame.Visible = false
	else
		submitButton.Text = "Wrong Key!"
		wait(1)
		submitButton.Text = "Submit"
	end
end)

copyButton.MouseButton1Click:Connect(function()
	local link = "https://link-center.net/423364/horizon-key"
	
	if setclipboard then
		setclipboard(link)
		copyButton.Text = "Copied!"
	else
		copyButton.Text = "Clipboard unsupported"
	end

	wait(1.5)
	copyButton.Text = "Get Key [Clipboard]"
end)
